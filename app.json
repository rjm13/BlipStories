{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "name": "BlipStories",
    "scheme": "blip",
    "slug": "BlipStories",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/blipicon.png",
    "splash": {
      "image": "./assets/images/blankprofile.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.martianmedia.blip"
    },
    "android": {
      "package": "com.martianmedia.blip",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/blipicon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/images/blipicon.png"
    }
  }
}
